<edit></edit>\n                                    </el-icon>\n                                </div>\n                            </div>\n                        </el-upload>\n                    </div>\n                    <el-descriptions :column=\"1\" :size=\"large\" border style=\"min-width: 500px\" title=\"\">\n                        <el-descriptions-item>\n                            <template #label>\n                                <div>\n                                    <el-icon>\n                                        <user/>\n                                    </el-icon>\n                                    用户名\n                                </div>\n                            </template>\n                            {{ username }}\n                        </el-descriptions-item>\n                        <el-descriptions-item>\n                            <template #label>\n                                <div>\n                                    <el-icon>\n                                        <location/>\n                                    </el-icon>\n                                    姓名\n                                </div>\n                            </template>\n                            {{ name }}\n                        </el-descriptions-item>\n                        <el-descriptions-item>\n                            <template #label>\n                                <div>\n                                    <el-icon>\n                                        <tickets/>\n                                    </el-icon>\n                                    性别\n                                </div>\n                            </template>\n                            {{ gender }}\n                        </el-descriptions-item>\n                        <el-descriptions-item>\n                            <template #label>\n                                <div>\n                                    <el-icon>\n                      