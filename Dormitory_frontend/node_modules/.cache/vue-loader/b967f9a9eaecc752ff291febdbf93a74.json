=\"width: 600px; margin-left: 30px; position: relative\">\n                    <div>\n                        <el-upload :on-success=\"uploadSuccess\" :show-file-list=\"false\"\n                                   action=\"http://localhost:9090/files/upload/\"\n                                   class=\"upload-demo\"\n                        >\n                            <div class=\"AvatarDiv\">\n                                <el-avatar v-if=\"!image\" icon=\"UserFilled\" style=\"width: 80px; height: 80px\"></el-avatar>\n                                <img v-if=\"image\" :src=\"'data:image;base64,' + image\" :style=\"imgDisplay\"\n                                     style=\"width: 80px; height: 80px; border-radius: 40px\"/>\n                                <div class=\"editImg\">\n                                    更换头像\n                                    <el-icon color=\"color\">\n                                        <edit></edit>\n                                    </el-icon>\n                                </div>\n                            </div>\n                        </el-upload>\n                    </div>\n                    <el-descriptions :column=\"1\" :size=\"large\" border style=\"min-width: 500px\" title=\"\">\n                        <el-descriptions-item>\n                            <template #label>\n                                <div>\n                                    <el-icon>\n                                        <user/>\n                                    </el-icon>\n                                    用户名\n                                </div>\n                            </template>\n                            {{ username }}\n                        </el-descriptions-item>\n                        <el-descriptions-item>\n                            <template #label>\n                                <div>\n                                    <el-icon>\n                                        <location/>\n                                    </el-icon>\n                                    姓名\n                                </div>\n                            </template>\n                            {{ name }}\n                        </el-descriptions-item>\n                        <el-descriptions-item>\n                            <template #label>\n                                <div>\n                                    <el-icon>\n                                        <tickets/>\n                                    </el-icon>\n                                    性别\n                                </div>\n                            </template>\n                            {{ gender }}\n                        </el-descriptions-item>\n                        <el-descriptions-item>\n                            <template #label>\n                                <div>\n                                    <el-icon>\n                                        <office-building/>\n                                    </el-icon>\n                                    年龄\n                                </div>\n                            </template>\n                            {{ age }}\n                        </el-descriptions-item>\n                        <el-descriptions-item>\n                            <template #label>\n                                <div>\n                                    <el-icon>\n                                        <iphone/>\n                                    </el-icon>\n                                    手机号\n                                </div>\n                            </template>\n                            {{ phoneNum }}\n                        </el-descriptions-item>\n                        <el-descriptions-item>\n                            <template #label>\n                                <div>\n                                    <el-icon>\n                                        <office-building/>\n                                    </el-icon>\n                                    邮箱\n                                </div>\n                            </template>\n                            {{ email }}\n                        </el-descriptions-item>\n                    </el-descriptions>\n                    <el-tooltip content=\"修改信息\" placement=\"bottom\">\n                        <el-button icon=\"Edit\" size=\"large\" style=\"margin-top: 30px; width: 80px\" type=\"primary\"\n                                   @click=\"Edit\">\n                        </el-button>\n                    </el-tooltip>\n                </div>\n                <!-- <div class=\"img\" style=\"margin-left: 130px \">\n                    <img alt=\"\" src=\"\"/>\n                </div> -->\n            </div>\n            <div>\n                <!--      弹窗-->\n                <el-dialog v-model=\"dialogVisible\" title=\"操作\" width=\"30%\" @close=\"cancel\">\n                    <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"120px\">\n                        <el-form-item label=\"账号\" prop=\"username\">\n                            <el-input v-model=\"form.username\" disabled style=\"width: 80%\"></el-input>\n                        </el-form-item>\n                        <el-form-item label=\"密码\" prop=\"password\">\n                            <el-input v-model=\"form.password\" :disabled=\"disabled\" :show-password=\"showpassword\"\n                                      show-password\n                                      style=\"width: 80%\"></el-input>\n                            <el-tooltip content=\"修改密码\" placement=\"right\">\n                                <el-icon size=\"large\" style=\"margin-left: 5px; cursor: pointer\" @click=\"EditPass\">\n                                    <edit/>\n                                </el-icon>\n                            </el-tooltip>\n                        </el-form-item>\n                        <el-form-item :style=\"display\" label=\"确认密码\" prop=\"checkPass\">\n                            <el-input v-model=\"form.checkPass\" show-password style=\"width: 80%\"></el-input>\n                        </el-form-item>\n                        <el-form-item label=\"姓名\" prop=\"name\">\n                            <el-input v-model=\"form.name\" style=\"width: 80%\"></el-input>\n                        </el-form-item>\n                        <el-form-item label=\"性别\" prop=\"gender\">\n                            <el-radio v-model=\"form.gender\" label=\"男\">男</el-radio>\n                            <el-radio v-model=\"form.gender\" label=\"女\">女</el-radio>\n                        </el-form-item>\n                        <el-form-item label=\"年龄\" prop=\"age\">\n                            <el-input v-model.number=\"form.age\" style=\"width: 80%\"></el-input>\n                        </el-form-item>\n                        <el-form-item label=\"手机号\" prop=\"phoneNum\">\n                            <el-input v-model.number=\"form.phoneNum\" style=\"width: 80%\"></el-input>\n                        </el-form-item>\n                        <el-form-item label=\"邮箱地址\" prop=\"email\">\n                            <el-input v-model=\"form.email\" style=\"width: 80%\"></el-input>\n                        </el-form-item>\n                    </el-form>\n                    <template #footer>\n            <span class=\"dialog-footer\">\n              <el-button @click=\"cancel\">取 消</el-button>\n              <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n            </span>\n                    </template>\n                </el-dialog>\n            </div>\n        </el-card>\n    </div>\n</template>\n<script src=\"@/assets/js/SelfInfo.js\"></script>\n<style scoped>@import '../assets/css/SelfInfo.css';\n\n.login-container {\n    position: fixed;\n    height: 100%;\n    width: 100%;\n    top: 0;\n    left: 0;\n    background: linear-gradient(\n            135deg,\n            hsl(170deg, 80%, 70%),\n            hsl(190deg, 80%, 70%),\n            hsl(250deg, 80%, 70%),\n            hsl(320deg, 80%, 70%),\n            hsl(320deg, 80%, 70%),\n            hsl(250deg, 80%, 70%),\n            hsl(190deg, 80%, 70%),\n            hsl(190deg, 80%, 70%),\n            hsl(170deg, 80%, 70%)\n    );\n    background-size: 600%;\n    animation: myanimation 15s linear infinite;\n}\n\n@keyframes myanimation {\n    0% {\n        background-position: 0 0;\n    }\n    100% {\n        background-position: 100% 100%;\n    }\n}</style>"]}]}